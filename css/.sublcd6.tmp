*{
	margin: 0;
	padding: 0;
}
.nav-top{
	display: flex;
	width: 100%;
	justify-content: space-between;
	position: absolute;
	top: 10px;
	
}
.nav-top .nav-top-left{
	margin-left: 3%;
}
.nav-top .nav-top-left p{
	
}
.nav-top .nav-top-right a{
	
	text-decoration: none;
}
.nav-top .nav-top-right{
	display: flex;
	justify-content: space-around;
	width: 100px;
	margin-right: 3%;
}
.nav-top-right .registe{
	margin-right: 1%;
}
.banner_box{
	position: relative; 
	width: 100%;
}



.content{
	width: 100%;
	position: absolute;top: 50px;
}
.container{
	width: 100%;
	display: flex;
}
.container .container-left{
	-webkit-box-pack: center;
    -webkit-box-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
    vertical-align: middle;
    background-color: rgb(0, 0, 0);
    position: relative;
    width: 49.7%; 
    overflow: hidden !important;
    border-width: 1px !important;
    border-style: solid !important;
}
.container-left img{
	width: 100%;
	transition: ease .5s;
	transform: scale(1.05);
    
}
.container-left img:hover{
	transform: scale(1.1);
    transition: -ms-transform 450ms 
    cubic-bezier(0.645, 0.045, 0.355, 1),
     -webkit-transform 450ms
      cubic-bezier(0.645, 0.045, 0.355, 1),
       transform 450ms cubic-bezier(0.645, 0.045, 0.355, 1)
        !important;
}
.left-img{
	width: 99%;
}





