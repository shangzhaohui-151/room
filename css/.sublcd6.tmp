.bottom{
	position: absolute;
	margin-top: 40%;
	display: flex;
	height: 500px;
	width: 100%;
	justify-content: center;
}
.bottom .left{
	margin-right: 50px;
}
.bottom .right{
	margin-left: 50px;
	width: 25%;
}
.bill{
	border: 1px solid #e4e4e4 !important;
	width: 100%;
	height: 90%;
	padding: 10%;
	box-sizing: border-box;
}
.bill .tag{
	color: rgb(72, 72, 72) !important;
	font-size: 25px;
}
.gaps{
	margin-bottom: 10px;
}
.gaps input{
	width: 100%;
	height: 35px;
	border-radius: 2px;
	outline: none;
	-moz-appearance:none;
	border: 1px solid rgb(219, 213, 213) !important;
}
.btn{
	width: 150px;
	height: 30px;
	margin-left: 25%;
	background: #3C3C3C;
	color: white;
	-moz-appearance:none;
	transition: ease .5s;
}
.gaps .birth{
	color: rgb(72, 72, 72) !important;
	font-size: 12px;
	font-weight: 600;
}
.btn:hover{
	background: black;
}
.gaps .boot{
	color: rgb(72, 72, 72) !important;
	font-size: 18px;
	font-weight: 600;
}